Category = employeeCategory;\r\n            this.branchManagement = branchManagement;\r\n            this.payrollManagementService = payrollManagementService;\r\n            this.route = route;\r\n            this.accountManagementService = accountManagementService;\r\n            this.tenantDefaultService = tenantDefaultService;\r\n            this.userManagementService = userManagementService;\r\n            this.renderer = renderer;\r\n            this.isLoading = true;\r\n            this.isView = false;\r\n            this.complete = true;\r\n            this.isGenerate = false;\r\n            this.errTimekeeping = false;\r\n            this.errPosting = false;\r\n            this.isSearch = false;\r\n            this.isPost = false;\r\n            this.defaultNavActiveId = 1;\r\n            this.tkList = [];\r\n            this.category = [];\r\n            this.branch = [];\r\n            this.confidential = [];\r\n            this.adjList = [];\r\n            this.adjHList = [];\r\n            this.payHList = [];\r\n            this.tagType = [];\r\n            this.branchlist = [];\r\n            this.company = [];\r\n            this.department = [];\r\n            this.employee = [];\r\n            this.nameList = [];\r\n            this.typeList = [];\r\n            this.genList = [];\r\n            this.postedList = [];\r\n            this.summary = [];\r\n            this.overtime = [];\r\n            this.addition = [];\r\n            this.deduction = [];\r\n            this.tkView = [];\r\n            this.payList = [];\r\n            this.pipe = new common[\"e\" /* DatePipe */]('en-US');\r\n            this.dtOptions = [];\r\n            this.dtTriggerPosting = new Subject[\"a\" /* Subject */]();\r\n            this.dtTriggerPosted = new Subject[\"a\" /* Subject */]();\r\n        }\r\n        ngOnInit() {\r\n            this.listenerFn = this.renderButton();\r\n            this.id = this.route.snapshot.paramMap.get('id');\r\n            this.payroll